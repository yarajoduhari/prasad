library 'default@master'

pipeline {
  agent {
    kubernetes {
      yamlFile 'kubernetespod.yml'
    }
   }
    stages {
    stage('Init') {
      steps {
        script {
          // Build the jars
          git 'https://github.com/yarajoduhari/maven-standalone-application.git'
          // word length checker  
          branchlint()
          }
        }
      }
    }
    stage('maven') {
      steps{
        sh "mvn clean install"
        }
      }
  }
